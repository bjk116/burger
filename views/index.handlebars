<div class = "row">
	<div class = "col-m-5">
		<h2>Burgers to Eat</h2>
		<ul>
			{{#each hamburger}}
				{{#unless this.devoured}}
				<li> {{burger_name}}</li>
				{{/unless}}
			{{/each}}
		</ul>
	</div>
	<div class = "col-m-5">
		<h2>Burgers already Eateded!</h2>
		<ul>
			{{#each hamburger}}
				{{#if this.devoured}}
					<li>{{burger_name}}</li>
				{{/if}}
			{{/each}}
		</ul>
	</div>
</div>